{
  "name": "jardiscore/dotenv",
  "version": "1.0.0",
  "description": "Advanced functions dotEnv tool for reading .env files for standard global and protected contexts",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Jardis Core Development",
      "email": "jardisCore@headgent.dev"
    }
  ],
  "require": {
    "php": ">=8.2",
    "jardispsr/dotenv": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "phpstan/phpstan": "^2.0.4",
    "squizlabs/php_codesniffer": "^3.11.2"
  },
  "autoload": {
    "psr-4": {
      "JardisCore\\DotEnv\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JardisCore\\DotEnv\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "echo '#!/usr/bin/env bash' > .git/hooks/pre-commit",
      "echo 'bash ./support/pre-commit-hook.sh' >> .git/hooks/pre-commit",
      "chmod +x .git/hooks/pre-commit"
    ]
  },
  "homepage": "https://github.com/jardisCore/dotenv",
  "support": {
    "issues": "https://github.com/jardisCore/dotenv/issues",
    "email": "jardisCore@headgent.dev"
  },
  "keywords": ["dotenv", "env", "environment", "loader", "context", "domain driven design", "jardisCore", "jardispsr", "jardis"]
}
